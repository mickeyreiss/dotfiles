[alias]
	st = status
	staged = diff --cached
	co = checkout
	br = branch
	pul = pull
	dif = diff
[help]
	autoCorrect = 10
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/local/bin/vim
	pager = less -F -X
[include]
  path = ~/.gitconfig_local
[rebase]
	autosquash = true
[credential]
	helper = osxkeychain
[user]
	email = mickeyreiss@gmail.com
	name = Mickey Reiss
[difftool]
	prompt = true
[merge]
	tool = opendiff
